
#include <bits/stdc++.h>

using namespace std;

void PrintNums(int ind,vector<int>&ds,int arr[],int n)
{
    if(ind==n)
    {
        for(auto it : ds)
        {
            cout<<it<<" ";
        }
        
        cout<<endl;
    }
    
    ds.push_back(arr[ind]);
    PrintNums(ind+1,ds,arr,n);
    ds.pop_back(arr[ind]);
    
    PrintNums(ind+1,ds,arr,n);
}
int main()
{

  int arr[]={3,1,2};
  int n = 3;
  
  vector<int>ds;
  PrintNums(0,ds,arr,n);
    
  
    
    return 0;
}
