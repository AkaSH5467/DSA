#include <iostream>

using namespace std;

struct Node{
    int data;
    Node *link;
};

Node *head =NULL;

void InsBEg(int d)
{
    Node *ptr=new Node();
    ptr->data=d;
    ptr->link=head;
    head=ptr;
}
void RevList()
{
    Node *prev,*curr,*next;
    curr=head;
    prev=NULL;
    
    while(curr!=NULL){
        next=curr->link;
        curr->link=prev;
        prev=curr;
        curr=next;
    }
    head=prev;
}
