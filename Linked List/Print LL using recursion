int main()
{
    int m,n;
    cout<<"Enter the number of elements to be included"<<endl;
    cin>>m;
    for(int i=0;i<=m-1;i++)
    {
        cout<<"Enter the "<<i<<"th Number"<<endl;
        cin>>n;
        InsBEg(n);
    }

    
    
   RevPrint(head);

    return 0;
}
