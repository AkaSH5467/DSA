#include<iostream>
using namespace std;

int main()
{
    int n,f=0,t,p;
    int a[100];
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        
    }
    
    cout<<"Enter the number to be replaced  :";
    cin>>p;
    
    cout<<"Enter the number to be addded  :";
    cin>>t;
    
    for(int i=0;i<n;i++)
    {
        if(a[i]==p)
        {
            a[i]=t;
            f++;
            
            
        }
        
    }
     
    
    
   if(f==0)
   {
       cout<<"Not found,Programme terminated";
   }
    
   else{
        cout<<"Number found and replaced, New array is"<<endl;
        for(int i=0;i<n;i++)
        {
            cout<<a[i]<<" ";
        }
        
    }
}
