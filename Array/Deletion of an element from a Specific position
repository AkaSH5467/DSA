
#include<iostream>
using namespace std;

int main()
{
    int a[100];
    int n;
    int k;
    
    
    cout<<"Enetr the element";
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    cout<<"Enter the position to be deleted ";
    cin>>k;
    
    
    if(k>n)
    {
        cout<<"Invalid input";
    }
  else{
        
    
    for(int i=k-1;i<=n;i++)
    {
        a[i]=a[i+1];
    }
    
    
    
        cout<<"New array is"<<endl;
        for(int i=0;i<n-1;i++)
        {
            cout<<a[i]<<" ";
        }
    }
}
