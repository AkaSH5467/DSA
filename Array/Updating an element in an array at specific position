#include<iostream>
using namespace std;

int main()
{
    int n,t,p;
    int a[100];
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        
    }
    
    cout<<"Enter the position to be replaced  :";
    cin>>p;
    
    cout<<"Enter the number to be added  :";
    cin>>t;
    
     a[p-1]=t;
    
    
   if(p>n)
   {
       cout<<"Array Limited Exceded";
   }
    
   else{
        cout<<" replaced, New array is"<<endl;
        for(int i=0;i<n;i++)
        {
            cout<<a[i]<<" ";
        }
        
    }
}
