
#include <iostream>

using namespace std;

int fact(int n)
{
    if(n==0 || n==1)
    {
        return 1;
    }
    
    
    return n*fact(n-1);
}

int main()
{
    int n;
    cin>>n;
    /*int fact=1;
    for(int i=2;i<=n;i++)
    {
        fact=fact*i;
    }
    
    cout<<fact<<endl;*/
    
    cout<<fact(n)<<endl;
    
}
